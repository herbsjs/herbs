{
  "name": "@herbsjs/herbs",
  "version": "1.0.1",
  "description": "A domain-first framework. Code your domain and your infrastructure will follow",
  "main": "./src/herbs.js",
  "scripts": {
    "test": "mocha --timeout 999999 --colors --recursive ./test",
    "lint": "npx eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npx eslint \"**/*.{js,jsx}\" --fix",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "type": "module",
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 12.16.2"
  },
  "keywords": [
    "entity",
    "value object",
    "validation",
    "error code only"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.4"
  },
  "author": "Jhonathan de Souza Soares",
  "license": "MIT",
  "dependencies": {
    "buchu": "^1.0.15",
    "gotu": "^1.0.21",
    "suma": "^1.0.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herbsjs/herbs.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
